{
  "name": "eternal-timer",
  "version": "1.3.0",
  "description": "timer for node.js package",
  "scripts": {
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "build": "tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json && node scripts/rename-cjs.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SUKEsann2000/eternal-timer.git"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "type": "module",
  "bugs": {
    "url": "https://github.com/SUKEsann2000/eternal-timer/issues"
  },
  "homepage": "https://github.com/SUKEsann2000/eternal-timer#readme",
  "devDependencies": {
    "@types/node": "^25.0.2",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "types": "dist/esm/index.d.ts",
  "dependencies": {
    "uuid": "^13.0.0"
  },
  "files": [ "dist", "src" ]
}
